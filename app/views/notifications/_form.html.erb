<%= form_for([@student,@notification], ) do |f| %>
    <div class="field ">
      Falta <br>
      <%= collection_select(:notification,:lackcode_id, Lackcode.all ,:id,:name,{:prompt => 'Tipo de falta'}, {class: "btn btn-raised dropdown-toggle"}) %>
    </div>

    <div >
      Descripcion <br>
      <%= f.text_area :description %>
    </div>

    <div class="actions">
      <%= f.submit class:"btn btn-raised btn-success" %>
    </div>
<% end %>
