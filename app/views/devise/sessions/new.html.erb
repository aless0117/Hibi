
<div class="container">
 <div class="row">
    <div class="col-md-6 contenedor">   </div>
      <div class="col-md-6 login">
        <h2>Log in</h2>
        <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
          <div class="form-group label-floating">
            <%= f.label :email, id:"i5", class:"control-label" %><br />
            <%= f.email_field :email, autofocus: true, class:"form-control", id:"i5" %>
          </div>

          <div class="form-group label-floating">
            <%= f.label :password, id:"i5", class:"control-label" %><br />
            <%= f.password_field :password, autocomplete: "off", class:"form-control", id:"i5" %>
          </div>
        <!--  <% if devise_mapping.rememberable? -%>
            <div class="field">
              <%= f.check_box :remember_me %>
              <%= f.label :remember_me %>
            </div>
          <% end -%> -->
          <div>
            <i class="material-icons">send<%= f.submit "Acceder", class:"boton" %></i>
          </div>
        <% end %>
        <%= render "devise/shared/links" %>
      </div>
 </div>
</div>
